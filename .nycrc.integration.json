{
	"extends": "@istanbuljs/nyc-config-typescript",
	"all": true,
	"check-coverage": false,
	"reporter": [
		"text",
		"html",
		"lcov"
	],
	"include": [
		"src/**/*.ts",
		"!src/**/*.test.ts",
		"!src/**/__tests__/**",
		"!src/test/**",
		"!src/**/*.d.ts"
	],
	"exclude": [
		"coverage/**",
		"dist/**",
		"out/**",
		"webview-ui/**",
		"node_modules/**",
		"**/*.test.ts",
		"**/__tests__/**",
		"src/test/**"
	],
	"extension": [
		".ts"
	],
	"temp-dir": "./coverage/.nyc_output"
}
