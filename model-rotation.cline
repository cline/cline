# Key/Model Pairs Configuration
keys:
  - api_key: YOUR_API_KEY_1
    model: gpt-4
    source_url: https://api.openai.com/v1
    cooldown: 300 # 5 minutes in seconds
    
  - api_key: YOUR_API_KEY_2  
    model: gpt-3.5-turbo
    source_url: https://api.openai.com/v1
    cooldown: 300

# Rotation Scheme Configuration
rotation:
  scheme: round-robin # Default rotation scheme
  # Future schemes can be added here
  
# Global Configuration
config:
  default_cooldown: 300 # 5 minutes in seconds
  max_retries: 3
  timeout: 30 # seconds
