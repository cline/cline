  "text": "{\"request\":\"[read_file for 'src/core/sliding-window/index.ts', 'src/core/tools/attemptCompletionTool.ts', 'src/shared/modes.ts'] Result:\\n\\n<files>\\n<file><path>src/core/sliding-window/index.ts</path>\\n<content lines=\\\"1-150\\\">\\n  1 | import { Anthropic } from \\\"@anthropic-ai/sdk\\\"\\n  2 | \\n  3 | import { TelemetryService } from \\\"@roo-code/telemetry\\\"\\n  4 | \\n  5 | import { ApiHandler } from \\\"../../api\\\"\\n  6 | import { summarizeConversation, SummarizeResponse } from \\\"../condense\\\"\\n  7 | import { ApiMessage } from \\\"../task-persistence/apiMessages\\\"\\n  8 | \\n  9 | /**\\n 10 |  * Default percentage of the context window to use as a buffer when deciding when to truncate\\n 11 |  */\\n 12 | export const TOKEN_BUFFER_PERCENTAGE = 0.1\\n 13 | \\n 14 | /**\\n 15 |  * Counts tokens for user content using the provider's token counting implementation.\\n 16 |  *\\n 17 |  * @param {Array<Anthropic.Messages.ContentBlockParam>} content - The content to count tokens for\\n 18 |  * @param {ApiHandler} apiHandler - The API handler to use for token counting\\n 19 |  * @returns {Promise<number>} A promise resolving to the token count\\n 20 |  */\\n 21 | export async function estimateTokenCount(\\n 22 | \\tcontent: Array<Anthropic.Messages.ContentBlockParam>,\\n 23 | \\tapiHandler: ApiHandler,\\n 24 | ): Promise<number> {\\n 25 | \\tif (!content || content.length === 0) return 0\\n 26 | \\treturn apiHandler.countTokens(content)\\n 27 | }\\n 28 | \\n 29 | /**\\n 30 |  * Truncates a conversation by removing a fraction of the messages.\\n 31 |  *\\n 32 |  * The first message is always retained, and a specified fraction (rounded to an even number)\\n 33 |  * of messages from the beginning (excluding the first) is removed.\\n 34 |  *\\n 35 |  * @param {ApiMessage[]} messages - The conversation messages.\\n 36 |  * @param {number} fracToRemove - The fraction (between 0 and 1) of messages (excluding the first) to remove.\\n 37 |  * @param {string} taskId - The task ID for the conversation, used for telemetry\\n 38 |  * @returns {ApiMessage[]} The truncated conversation messages.\\n 39 |  */\\n 40 | export function truncateConversation(messages: ApiMessage[], fracToRemove: number, taskId: string): ApiMessage[] {\\n 41 | \\tTelemetryService.instance.captureSlidingWindowTruncation(taskId)\\n 42 | \\tconst truncatedMessages = [messages[0]]\\n 43 | \\tconst rawMessagesToRemove = Math.floor((messages.length - 1) * fracToRemove)\\n 44 | \\tconst messagesToRemove = rawMessagesToRemove - (rawMessagesToRemove % 2)\\n 45 | \\tconst remainingMessages = messages.slice(messagesToRemove + 1)\\n 46 | \\ttruncatedMessages.push(...remainingMessages)\\n 47 | \\n 48 | \\treturn truncatedMessages\\n 49 | }\\n 50 | \\n 51 | /**\\n 52 |  * Conditionally truncates the conversation messages if the total token count\\n 53 |  * exceeds the model's limit, considering the size of incoming content.\\n 54 |  *\\n 55 |  * @param {ApiMessage[]} messages - The conversation messages.\\n 56 |  * @param {number} totalTokens - The total number of tokens in the conversation (excluding the last user message).\\n 57 |  * @param {number} contextWindow - The context window size.\\n 58 |  * @param {number} maxTokens - The maximum number of tokens allowed.\\n 59 |  * @param {ApiHandler} apiHandler - The API handler to use for token counting.\\n 60 |  * @param {boolean} autoCondenseContext - Whether to use LLM summarization or sliding window implementation\\n 61 |  * @param {string} systemPrompt - The system prompt, used for estimating the new context size after summarizing.\\n 62 |  * @returns {ApiMessage[]} The original or truncated conversation messages.\\n 63 |  */\\n 64 | \\n 65 | type TruncateOptions = {\\n 66 | \\tmessages: ApiMessage[]\\n 67 | \\ttotalTokens: number\\n 68 | \\tcontextWindow: number\\n 69 | \\tmaxTokens?: number | null\\n 70 | \\tapiHandler: ApiHandler\\n 71 | \\tautoCondenseContext: boolean\\n 72 | \\tautoCondenseContextPercent: number\\n 73 | \\tsystemPrompt: string\\n 74 | \\ttaskId: string\\n 75 | \\tcustomCondensingPrompt?: string\\n 76 | \\tcondensingApiHandler?: ApiHandler\\n 77 | }\\n 78 | \\n 79 | type TruncateResponse = SummarizeResponse & { prevContextTokens: number }\\n 80 | \\n 81 | /**\\n 82 |  * Conditionally truncates the conversation messages if the total token count\\n 83 |  * exceeds the model's limit, considering the size of incoming content.\\n 84 |  *\\n 85 |  * @param {TruncateOptions} options - The options for truncation\\n 86 |  * @returns {Promise<ApiMessage[]>} The original or truncated conversation messages.\\n 87 |  */\\n 88 | export async function truncateConversationIfNeeded({\\n 89 | \\tmessages,\\n 90 | \\ttotalTokens,\\n 91 | \\tcontextWindow,\\n 92 | \\tmaxTokens,\\n 93 | \\tapiHandler,\\n 94 | \\tautoCondenseContext,\\n 95 | \\tautoCondenseContextPercent,\\n 96 | \\tsystemPrompt,\\n 97 | \\ttaskId,\\n 98 | \\tcustomCondensingPrompt,\\n 99 | \\tcondensingApiHandler,\\n100 | }: TruncateOptions): Promise<TruncateResponse> {\\n101 | \\tlet error: string | undefined\\n102 | \\tlet cost = 0\\n103 | \\t// Calculate the maximum tokens reserved for response\\n104 | \\tconst reservedTokens = maxTokens || contextWindow * 0.2\\n105 | \\n106 | \\t// Estimate tokens for the last message (which is always a user message)\\n107 | \\tconst lastMessage = messages[messages.length - 1]\\n108 | \\tconst lastMessageContent = lastMessage.content\\n109 | \\tconst lastMessageTokens = Array.isArray(lastMessageContent)\\n110 | \\t\\t? await estimateTokenCount(lastMessageContent, apiHandler)\\n111 | \\t\\t: await estimateTokenCount([{ type: \\\"text\\\", text: lastMessageContent as string }], apiHandler)\\n112 | \\n113 | \\t// Calculate total effective tokens (totalTokens never includes the last message)\\n114 | \\tconst prevContextTokens = totalTokens + lastMessageTokens\\n115 | \\n116 | \\t// Calculate available tokens for conversation history\\n117 | \\t// Truncate if we're within TOKEN_BUFFER_PERCENTAGE of the context window\\n118 | \\tconst allowedTokens = contextWindow * (1 - TOKEN_BUFFER_PERCENTAGE) - reservedTokens\\n119 | \\n120 | \\tif (autoCondenseContext) {\\n121 | \\t\\tconst contextPercent = (100 * prevContextTokens) / contextWindow\\n122 | \\t\\tif (contextPercent >= autoCondenseContextPercent || prevContextTokens > allowedTokens) {\\n123 | \\t\\t\\t// Attempt to intelligently condense the context\\n124 | \\t\\t\\tconst result = await summarizeConversation(\\n125 | \\t\\t\\t\\tmessages,\\n126 | \\t\\t\\t\\tapiHandler,\\n127 | \\t\\t\\t\\tsystemPrompt,\\n128 | \\t\\t\\t\\ttaskId,\\n129 | \\t\\t\\t\\tprevContextTokens,\\n130 | \\t\\t\\t\\ttrue, // automatic trigger\\n131 | \\t\\t\\t\\tcustomCondensingPrompt,\\n132 | \\t\\t\\t\\tcondensingApiHandler,\\n133 | \\t\\t\\t)\\n134 | \\t\\t\\tif (result.error) {\\n135 | \\t\\t\\t\\terror = result.error\\n136 | \\t\\t\\t\\tcost = result.cost\\n137 | \\t\\t\\t} else {\\n138 | \\t\\t\\t\\treturn { ...result, prevContextTokens }\\n139 | \\t\\t\\t}\\n140 | \\t\\t}\\n141 | \\t}\\n142 | \\n143 | \\t// Fall back to sliding window truncation if needed\\n144 | \\tif (prevContextTokens > allowedTokens) {\\n145 | \\t\\tconst truncatedMessages = truncateConversation(messages, 0.5, taskId)\\n146 | \\t\\treturn { messages: truncatedMessages, prevContextTokens, summary: \\\"\\\", cost, error }\\n147 | \\t}\\n148 | \\t// No truncation or condensation needed\\n149 | \\treturn { messages, summary: \\\"\\\", cost, prevContextTokens, error }\\n150 | }\\n</content>\\n</file>\\n<file><path>src/core/tools/attemptCompletionTool.ts</path>\\n<content lines=\\\"1-164\\\">\\n  1 | import Anthropic from \\\"@anthropic-ai/sdk\\\"\\n  2 | \\n  3 | import { TelemetryService } from \\\"@roo-code/telemetry\\\"\\n  4 | \\n  5 | import { Task } from \\\"../task/Task\\\"\\n  6 | import {\\n  7 | \\tToolResponse,\\n  8 | \\tToolUse,\\n  9 | \\tAskApproval,\\n 10 | \\tHandleError,\\n 11 | \\tPushToolResult,\\n 12 | \\tRemoveClosingTag,\\n 13 | \\tToolDescription,\\n 14 | \\tAskFinishSubTaskApproval,\\n 15 | } from \\\"../../shared/tools\\\"\\n 16 | import { formatResponse } from \\\"../prompts/responses\\\"\\n 17 | import { type ExecuteCommandOptions, executeCommand } from \\\"./executeCommandTool\\\"\\n 18 | import { EXPERIMENT_IDS, experiments, experimentDefault } from \\\"../../shared/experiments\\\"\\n 19 | \\n 20 | export async function attemptCompletionTool(\\n 21 | \\tcline: Task,\\n 22 | \\tblock: ToolUse,\\n 23 | \\taskApproval: AskApproval,\\n 24 | \\thandleError: HandleError,\\n 25 | \\tpushToolResult: PushToolResult,\\n 26 | \\tremoveClosingTag: RemoveClosingTag,\\n 27 | \\ttoolDescription: ToolDescription,\\n 28 | \\taskFinishSubTaskApproval: AskFinishSubTaskApproval,\\n 29 | ) {\\n 30 | \\tconst result: string | undefined = block.params.result\\n 31 | \\tconst command: string | undefined = block.params.command\\n 32 | \\n 33 | \\ttry {\\n 34 | \\t\\tconst lastMessage = cline.clineMessages.at(-1)\\n 35 | \\n 36 | \\t\\tif (block.partial) {\\n 37 | \\t\\t\\tif (command) {\\n 38 | \\t\\t\\t\\t// the attempt_completion text is done, now we're getting command\\n 39 | \\t\\t\\t\\t// remove the previous partial attempt_completion ask, replace with say, post state to webview, then stream command\\n 40 | \\n 41 | \\t\\t\\t\\t// const secondLastMessage = cline.clineMessages.at(-2)\\n 42 | \\t\\t\\t\\tif (lastMessage && lastMessage.ask === \\\"command\\\") {\\n 43 | \\t\\t\\t\\t\\t// update command\\n 44 | \\t\\t\\t\\t\\tawait cline.ask(\\\"command\\\", removeClosingTag(\\\"command\\\", command), block.partial).catch(() => {})\\n 45 | \\t\\t\\t\\t} else {\\n 46 | \\t\\t\\t\\t\\t// last message is completion_result\\n 47 | \\t\\t\\t\\t\\t// we have command string, which means we have the result as well, so finish it (doesnt have to exist yet)\\n 48 | \\t\\t\\t\\t\\tawait cline.say(\\\"completion_result\\\", removeClosingTag(\\\"result\\\", result), undefined, false)\\n 49 | \\n 50 | \\t\\t\\t\\t\\tTelemetryService.instance.captureTaskCompleted(cline.taskId)\\n 51 | \\t\\t\\t\\t\\tcline.emit(\\\"taskCompleted\\\", cline.taskId, cline.getTokenUsage(), cline.toolUsage)\\n 52 | \\n 53 | \\t\\t\\t\\t\\tawait cline.ask(\\\"command\\\", removeClosingTag(\\\"command\\\", command), block.partial).catch(() => {})\\n 54 | \\t\\t\\t\\t}\\n 55 | \\t\\t\\t} else {\\n 56 | \\t\\t\\t\\t// no command, still outputting partial result\\n 57 | \\t\\t\\t\\tawait cline.say(\\\"completion_result\\\", removeClosingTag(\\\"result\\\", result), undefined, block.partial)\\n 58 | \\t\\t\\t}\\n 59 | \\t\\t\\treturn\\n 60 | \\t\\t} else {\\n 61 | \\t\\t\\tif (!result) {\\n 62 | \\t\\t\\t\\tcline.consecutiveMistakeCount++\\n 63 | \\t\\t\\t\\tcline.recordToolError(\\\"attempt_completion\\\")\\n 64 | \\t\\t\\t\\tpushToolResult(await cline.sayAndCreateMissingParamError(\\\"attempt_completion\\\", \\\"result\\\"))\\n 65 | \\t\\t\\t\\treturn\\n 66 | \\t\\t\\t}\\n 67 | \\n 68 | \\t\\t\\tcline.consecutiveMistakeCount = 0\\n 69 | \\n 70 | \\t\\t\\tlet commandResult: ToolResponse | undefined\\n 71 | \\n 72 | \\t\\t\\t// Check if command execution is disabled via experiment\\n 73 | \\t\\t\\tconst state = await cline.providerRef.deref()?.getState()\\n 74 | \\t\\t\\tconst experimentsConfig = state?.experiments ?? experimentDefault\\n 75 | \\t\\t\\tconst isCommandDisabled = experiments.isEnabled(\\n 76 | \\t\\t\\t\\texperimentsConfig,\\n 77 | \\t\\t\\t\\tEXPERIMENT_IDS.DISABLE_COMPLETION_COMMAND,\\n 78 | \\t\\t\\t)\\n 79 | \\n 80 | \\t\\t\\tif (command && !isCommandDisabled) {\\n 81 | \\t\\t\\t\\tif (lastMessage && lastMessage.ask !== \\\"command\\\") {\\n 82 | \\t\\t\\t\\t\\t// Haven't sent a command message yet so first send completion_result then command.\\n 83 | \\t\\t\\t\\t\\tawait cline.say(\\\"completion_result\\\", result, undefined, false)\\n 84 | \\t\\t\\t\\t\\tTelemetryService.instance.captureTaskCompleted(cline.taskId)\\n 85 | \\t\\t\\t\\t\\tcline.emit(\\\"taskCompleted\\\", cline.taskId, cline.getTokenUsage(), cline.toolUsage)\\n 86 | \\t\\t\\t\\t}\\n 87 | \\n 88 | \\t\\t\\t\\t// Complete command message.\\n 89 | \\t\\t\\t\\tconst didApprove = await askApproval(\\\"command\\\", command)\\n 90 | \\n 91 | \\t\\t\\t\\tif (!didApprove) {\\n 92 | \\t\\t\\t\\t\\treturn\\n 93 | \\t\\t\\t\\t}\\n 94 | \\n 95 | \\t\\t\\t\\tconst executionId = cline.lastMessageTs?.toString() ?? Date.now().toString()\\n 96 | \\t\\t\\t\\tconst options: ExecuteCommandOptions = { executionId, command }\\n 97 | \\t\\t\\t\\tconst [userRejected, execCommandResult] = await executeCommand(cline, options)\\n 98 | \\n 99 | \\t\\t\\t\\tif (userRejected) {\\n100 | \\t\\t\\t\\t\\tcline.didRejectTool = true\\n101 | \\t\\t\\t\\t\\tpushToolResult(execCommandResult)\\n102 | \\t\\t\\t\\t\\treturn\\n103 | \\t\\t\\t\\t}\\n104 | \\n105 | \\t\\t\\t\\t// User didn't reject, but the command may have output.\\n106 | \\t\\t\\t\\tcommandResult = execCommandResult\\n107 | \\t\\t\\t} else {\\n108 | \\t\\t\\t\\tawait cline.say(\\\"completion_result\\\", result, undefined, false)\\n109 | \\t\\t\\t\\tTelemetryService.instance.captureTaskCompleted(cline.taskId)\\n110 | \\t\\t\\t\\tcline.emit(\\\"taskCompleted\\\", cline.taskId, cline.getTokenUsage(), cline.toolUsage)\\n111 | \\t\\t\\t}\\n112 | \\n113 | \\t\\t\\tif (cline.parentTask) {\\n114 | \\t\\t\\t\\tconst didApprove = await askFinishSubTaskApproval()\\n115 | \\n116 | \\t\\t\\t\\tif (!didApprove) {\\n117 | \\t\\t\\t\\t\\treturn\\n118 | \\t\\t\\t\\t}\\n119 | \\n120 | \\t\\t\\t\\t// tell the provider to remove the current subtask and resume the previous task in the stack\\n121 | \\t\\t\\t\\tawait cline.providerRef.deref()?.finishSubTask(result)\\n122 | \\t\\t\\t\\treturn\\n123 | \\t\\t\\t}\\n124 | \\n125 | \\t\\t\\t// We already sent completion_result says, an\\n126 | \\t\\t\\t// empty string asks relinquishes control over\\n127 | \\t\\t\\t// button and field.\\n128 | \\t\\t\\tconst { response, text, images } = await cline.ask(\\\"completion_result\\\", \\\"\\\", false)\\n129 | \\n130 | \\t\\t\\t// Signals to recursive loop to stop (for now\\n131 | \\t\\t\\t// cline never happens since yesButtonClicked\\n132 | \\t\\t\\t// will trigger a new task).\\n133 | \\t\\t\\tif (response === \\\"yesButtonClicked\\\") {\\n134 | \\t\\t\\t\\tpushToolResult(\\\"\\\")\\n135 | \\t\\t\\t\\treturn\\n136 | \\t\\t\\t}\\n137 | \\n138 | \\t\\t\\tawait cline.say(\\\"user_feedback\\\", text ?? \\\"\\\", images)\\n139 | \\t\\t\\tconst toolResults: (Anthropic.TextBlockParam | Anthropic.ImageBlockParam)[] = []\\n140 | \\n141 | \\t\\t\\tif (commandResult) {\\n142 | \\t\\t\\t\\tif (typeof commandResult === \\\"string\\\") {\\n143 | \\t\\t\\t\\t\\ttoolResults.push({ type: \\\"text\\\", text: commandResult })\\n144 | \\t\\t\\t\\t} else if (Array.isArray(commandResult)) {\\n145 | \\t\\t\\t\\t\\ttoolResults.push(...commandResult)\\n146 | \\t\\t\\t\\t}\\n147 | \\t\\t\\t}\\n148 | \\n149 | \\t\\t\\ttoolResults.push({\\n150 | \\t\\t\\t\\ttype: \\\"text\\\",\\n151 | \\t\\t\\t\\ttext: `The user has provided feedback on the results. Consider their input to continue the task, and then attempt completion again.\\\\n<feedback>\\\\n${text}\\\\n</feedback>`,\\n152 | \\t\\t\\t})\\n153 | \\n154 | \\t\\t\\ttoolResults.push(...formatResponse.imageBlocks(images))\\n155 | \\t\\t\\tcline.userMessageContent.push({ type: \\\"text\\\", text: `${toolDescription()} Result:` })\\n156 | \\t\\t\\tcline.userMessageContent.push(...toolResults)\\n157 | \\n158 | \\t\\t\\treturn\\n159 | \\t\\t}\\n160 | \\t} catch (error) {\\n161 | \\t\\tawait handleError(\\\"inspecting site\\\", error)\\n162 | \\t\\treturn\\n163 | \\t}\\n164 | }\\n</content>\\n</file>\\n<file><path>src/shared/modes.ts</path>\\n<content lines=\\\"1-374\\\">\\n  1 | import * as vscode from \\\"vscode\\\"\\n  2 | \\n  3 | import type {\\n  4 | \\tGroupOptions,\\n  5 | \\tGroupEntry,\\n  6 | \\tModeConfig,\\n  7 | \\tCustomModePrompts,\\n  8 | \\tExperimentId,\\n  9 | \\tToolGroup,\\n 10 | \\tPromptComponent,\\n 11 | } from \\\"@roo-code/types\\\"\\n 12 | \\n 13 | import { addCustomInstructions } from \\\"../core/prompts/sections/custom-instructions\\\"\\n 14 | \\n 15 | import { EXPERIMENT_IDS } from \\\"./experiments\\\"\\n 16 | import { TOOL_GROUPS, ALWAYS_AVAILABLE_TOOLS } from \\\"./tools\\\"\\n 17 | \\n 18 | export type Mode = string\\n 19 | \\n 20 | // Helper to extract group name regardless of format\\n 21 | export function getGroupName(group: GroupEntry): ToolGroup {\\n 22 | \\tif (typeof group === \\\"string\\\") {\\n 23 | \\t\\treturn group\\n 24 | \\t}\\n 25 | \\n 26 | \\treturn group[0]\\n 27 | }\\n 28 | \\n 29 | // Helper to get group options if they exist\\n 30 | function getGroupOptions(group: GroupEntry): GroupOptions | undefined {\\n 31 | \\treturn Array.isArray(group) ? group[1] : undefined\\n 32 | }\\n 33 | \\n 34 | // Helper to check if a file path matches a regex pattern\\n 35 | export function doesFileMatchRegex(filePath: string, pattern: string): boolean {\\n 36 | \\ttry {\\n 37 | \\t\\tconst regex = new RegExp(pattern)\\n 38 | \\t\\treturn regex.test(filePath)\\n 39 | \\t} catch (error) {\\n 40 | \\t\\tconsole.error(`Invalid regex pattern: ${pattern}`, error)\\n 41 | \\t\\treturn false\\n 42 | \\t}\\n 43 | }\\n 44 | \\n 45 | // Helper to get all tools for a mode\\n 46 | export function getToolsForMode(groups: readonly GroupEntry[]): string[] {\\n 47 | \\tconst tools = new Set<string>()\\n 48 | \\n 49 | \\t// Add tools from each group\\n 50 | \\tgroups.forEach((group) => {\\n 51 | \\t\\tconst groupName = getGroupName(group)\\n 52 | \\t\\tconst groupConfig = TOOL_GROUPS[groupName]\\n 53 | \\t\\tgroupConfig.tools.forEach((tool: string) => tools.add(tool))\\n 54 | \\t})\\n 55 | \\n 56 | \\t// Always add required tools\\n 57 | \\tALWAYS_AVAILABLE_TOOLS.forEach((tool) => tools.add(tool))\\n 58 | \\n 59 | \\treturn Array.from(tools)\\n 60 | }\\n 61 | \\n 62 | // Main modes configuration as an ordered array\\n 63 | export const modes: readonly ModeConfig[] = [\\n 64 | \\t{\\n 65 | \\t\\tslug: \\\"code\\\",\\n 66 | \\t\\tname: \\\"ðŸ’» Code\\\",\\n 67 | \\t\\troleDefinition:\\n 68 | \\t\\t\\t\\\"You are Roo, a highly skilled software engineer with extensive knowledge in many programming languages, frameworks, design patterns, and best practices.\\\",\\n 69 | \\t\\twhenToUse:\\n 70 | \\t\\t\\t\\\"Use this mode when you need to write, modify, or refactor code. Ideal for implementing features, fixing bugs, creating new files, or making code improvements across any programming language or framework.\\\",\\n 71 | \\t\\tgroups: [\\\"read\\\", \\\"edit\\\", \\\"browser\\\", \\\"command\\\", \\\"mcp\\\"],\\n 72 | \\t},\\n 73 | \\t{\\n 74 | \\t\\tslug: \\\"architect\\\",\\n 75 | \\t\\tname: \\\"ðŸ—ï¸ Architect\\\",\\n 76 | \\t\\troleDefinition:\\n 77 | \\t\\t\\t\\\"You are Roo, an experienced technical leader who is inquisitive and an excellent planner. Your goal is to gather information and get context to create a detailed plan for accomplishing the user's task, which the user will review and approve before they switch into another mode to implement the solution.\\\",\\n 78 | \\t\\twhenToUse:\\n 79 | \\t\\t\\t\\\"Use this mode when you need to plan, design, or strategize before implementation. Perfect for breaking down complex problems, creating technical specifications, designing system architecture, or brainstorming solutions before coding.\\\",\\n 80 | \\t\\tgroups: [\\\"read\\\", [\\\"edit\\\", { fileRegex: \\\"\\\\\\\\.md$\\\", description: \\\"Markdown files only\\\" }], \\\"browser\\\", \\\"mcp\\\"],\\n 81 | \\t\\tcustomInstructions:\\n 82 | \\t\\t\\t\\\"1. Do some information gathering (for example using read_file or search_files) to get more context about the task.\\\\n\\\\n2. You should also ask the user clarifying questions to get a better understanding of the task.\\\\n\\\\n3. Once you've gained more context about the user's request, you should create a detailed plan for how to accomplish the task. Include Mermaid diagrams if they help make your plan clearer.\\\\n\\\\n4. Ask the user if they are pleased with this plan, or if they would like to make any changes. Think of this as a brainstorming session where you can discuss the task and plan the best way to accomplish it.\\\\n\\\\n5. Once the user confirms the plan, ask them if they'd like you to write it to a markdown file.\\\\n\\\\n6. Use the switch_mode tool to request that the user switch to another mode to implement the solution.\\\",\\n 83 | \\t},\\n 84 | \\t{\\n 85 | \\t\\tslug: \\\"ask\\\",\\n 86 | \\t\\tname: \\\"â“ Ask\\\",\\n 87 | \\t\\troleDefinition:\\n 88 | \\t\\t\\t\\\"You are Roo, a knowledgeable technical assistant focused on answering questions and providing information about software development, technology, and related topics.\\\",\\n 89 | \\t\\twhenToUse:\\n 90 | \\t\\t\\t\\\"Use this mode when you need explanations, documentation, or answers to technical questions. Best for understanding concepts, analyzing existing code, getting recommendations, or learning about technologies without making changes.\\\",\\n 91 | \\t\\tgroups: [\\\"read\\\", \\\"browser\\\", \\\"mcp\\\"],\\n 92 | \\t\\tcustomInstructions:\\n 93 | \\t\\t\\t\\\"You can analyze code, explain concepts, and access external resources. Always answer the user's questions thoroughly, and do not switch to implementing code unless explicitly requested by the user. Include Mermaid diagrams when they clarify your response.\\\",\\n 94 | \\t},\\n 95 | \\t{\\n 96 | \\t\\tslug: \\\"debug\\\",\\n 97 | \\t\\tname: \\\"ðŸª² Debug\\\",\\n 98 | \\t\\troleDefinition:\\n 99 | \\t\\t\\t\\\"You are Roo, an expert software debugger specializing in systematic problem diagnosis and resolution.\\\",\\n100 | \\t\\twhenToUse:\\n101 | \\t\\t\\t\\\"Use this mode when you're troubleshooting issues, investigating errors, or diagnosing problems. Specialized in systematic debugging, adding logging, analyzing stack traces, and identifying root causes before applying fixes.\\\",\\n102 | \\t\\tgroups: [\\\"read\\\", \\\"edit\\\", \\\"browser\\\", \\\"command\\\", \\\"mcp\\\"],\\n103 | \\t\\tcustomInstructions:\\n104 | \\t\\t\\t\\\"Reflect on 5-7 different possible sources of the problem, distill those down to 1-2 most likely sources, and then add logs to validate your assumptions. Explicitly ask the user to confirm the diagnosis before fixing the problem.\\\",\\n105 | \\t},\\n106 | \\t{\\n107 | \\t\\tslug: \\\"orchestrator\\\",\\n108 | \\t\\tname: \\\"ðŸªƒ Orchestrator\\\",\\n109 | \\t\\troleDefinition:\\n110 | \\t\\t\\t\\\"You are Roo, a strategic workflow orchestrator who coordinates complex tasks by delegating them to appropriate specialized modes. You have a comprehensive understanding of each mode's capabilities and limitations, allowing you to effectively break down complex problems into discrete tasks that can be solved by different specialists.\\\",\\n111 | \\t\\twhenToUse:\\n112 | \\t\\t\\t\\\"Use this mode for complex, multi-step projects that require coordination across different specialties. Ideal when you need to break down large tasks into subtasks, manage workflows, or coordinate work that spans multiple domains or expertise areas.\\\",\\n113 | \\t\\tgroups: [],\\n114 | \\t\\tcustomInstructions:\\n115 | \\t\\t\\t\\\"Your role is to coordinate complex workflows by delegating tasks to specialized modes. As an orchestrator, you should:\\\\n\\\\n1. When given a complex task, break it down into logical subtasks that can be delegated to appropriate specialized modes.\\\\n\\\\n2. For each subtask, use the `new_task` tool to delegate. Choose the most appropriate mode for the subtask's specific goal and provide comprehensive instructions in the `message` parameter. These instructions must include:\\\\n    *   All necessary context from the parent task or previous subtasks required to complete the work.\\\\n    *   A clearly defined scope, specifying exactly what the subtask should accomplish.\\\\n    *   An explicit statement that the subtask should *only* perform the work outlined in these instructions and not deviate.\\\\n    *   An instruction for the subtask to signal completion by using the `attempt_completion` tool, providing a concise yet thorough summary of the outcome in the `result` parameter, keeping in mind that this summary will be the source of truth used to keep track of what was completed on this project.\\\\n    *   A statement that these specific instructions supersede any conflicting general instructions the subtask's mode might have.\\\\n\\\\n3. Track and manage the progress of all subtasks. When a subtask is completed, analyze its results and determine the next steps.\\\\n\\\\n4. Help the user understand how the different subtasks fit together in the overall workflow. Provide clear reasoning about why you're delegating specific tasks to specific modes.\\\\n\\\\n5. When all subtasks are completed, synthesize the results and provide a comprehensive overview of what was accomplished.\\\\n\\\\n6. Ask clarifying questions when necessary to better understand how to break down complex tasks effectively.\\\\n\\\\n7. Suggest improvements to the workflow based on the results of completed subtasks.\\\\n\\\\nUse subtasks to maintain clarity. If a request significantly shifts focus or requires a different expertise (mode), consider creating a subtask rather than overloading the current one.\\\",\\n116 | \\t},\\n117 | ] as const\\n118 | \\n119 | // Export the default mode slug\\n120 | export const defaultModeSlug = modes[0].slug\\n121 | \\n122 | // Helper functions\\n123 | export function getModeBySlug(slug: string, customModes?: ModeConfig[]): ModeConfig | undefined {\\n124 | \\t// Check custom modes first\\n125 | \\tconst customMode = customModes?.find((mode) => mode.slug === slug)\\n126 | \\tif (customMode) {\\n127 | \\t\\treturn customMode\\n128 | \\t}\\n129 | \\t// Then check built-in modes\\n130 | \\treturn modes.find((mode) => mode.slug === slug)\\n131 | }\\n132 | \\n